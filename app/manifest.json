{
    "name": "simple delishows",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "converts deli shows to an simpler format",
    "icons": {
        "128": "images/icon-128.png"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://delishows.to/",
                "http://delishows.to/?page=*"
            ],
            "css": [
                "css/style.css"
            ],
            "js": [
                "scripts/date.js",
                "scripts/jquery.min.js",
                "scripts/delishows.js"
            ]
        }
    ]
}